egory,\r\n    });\r\n  };\r\n\r\n  //Hàm này dùng để thay đổi state pagination.search\r\n  //Hàm này sẽ truyền xuống Component con và nhận dữ liệu từ Component con truyền lên\r\n  const handlerSearch = (value) => {\r\n    console.log(\"Value: \", value);\r\n    setPagination({\r\n      page: pagination.page,\r\n      count: pagination.count,\r\n      search: value,\r\n      category: pagination.category,\r\n    });\r\n  };\r\n\r\n  //Gọi hàm useEffect tìm tổng số sản phẩm để tính tổng số trang\r\n  //Và nó phụ thuộc và state pagination\r\n  useEffect(() => {\r\n    const fetchAllData = async () => {\r\n        let response = await ProductAPI.getAPI();\r\n      console.log(response);\r\n      console.log(category);\r\n      const data = response.filter(data => data.category === category)\r\n        setProducts(data);\r\n      // Nếu mà category === 'all' thì nó sẽ gọi hàm get tất cả sản phẩm\r\n      // Ngược lại thì nó sẽ gọi hàm pagination và phân loại sản phẩm\r\n      if (pagination.category === \"all\" || !pagination.category) {\r\n        setProducts(response);\r\n      } \r\n      //Tính tổng số trang = tổng số sản phẩm / số lượng sản phẩm 1 trang\r\n      const totalPage = Math.ceil(\r\n        parseInt(response?.length) / parseInt(pagination.count)\r\n      );\r\n      console.log(totalPage);\r\n      setTotalPage(totalPage);\r\n    };\r\n\r\n    fetchAllData();\r\n  }, [pagination]);\r\n\r\n  //Gọi hàm Pagination\r\n  useEffect(() => {\r\n    const fetchData = async () => {\r\n      const params = {\r\n        page: pagination.page,\r\n        count: pagination.count,\r\n        search: pagination.search,\r\n        category: pagination.category,\r\n      };\r\n\r\n      const query = queryString.stringify(params);\r\n      const newQuery = \"?\" + query;\r\n      const response = await ProductAPI.getPagination(newQuery);\r\n      setProducts(response);\r\n    };\r\n\r\n    fetchData();\r\n  }, [pagination]);\r\n\r\n  //Hàm này dùng để thay đổi state pagination.category\r\n  const handlerCategory = (value) => {\r\n    console.log(value);\r\n    navigate(`/shop?category=${value}#`)\r\n\r\n    setPagination({\r\n      page: pagination.page,\r\n      count: pagination.count,\r\n      search: pagination.search,\r\n      category: value,\r\n    });\r\n  };\r\n\r\n  return (\r\n    <div className=\"container\">\r\n      <section className=\"py-5 bg-light\">\r\n        <div className=\"container\">\r\n          <div className=\"row px-4 px-lg-5 py-lg-4 align-items-center\">\r\n            <div className=\"col-lg-6\">\r\n              <h1 className=\"h2 text-uppercase mb-0\">Shop</h1>\r\n            </div>\r\n            <div className=\"col-lg-6 text-lg-right\">\r\n              <nav aria-label=\"breadcrumb\">\r\n                <ol className=\"breadcrumb justify-content-lg-end mb-0 px-0\">\r\n                  <li className=\"breadcrumb-item active\" aria-current=\"page\">\r\n                    Shop\r\n                  </li>\r\n                </ol>\r\n              </nav>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n\r\n      {/* -------------Modal Product----------------- */}\r\n      {products &&\r\n        products.map((value) => (\r\n          <div\r\n            className=\"modal fade show\"\r\n            id={`product_${value._id}`}\r\n            key={value._id}\r\n          >\r\n            <div\r\n              className=\"modal-dialog modal-lg modal-dialog-centered\"\r\n              role=\"document\"\r\n            >\r\n              <div className=\"modal-content\">\r\n                <div className=\"modal-body p-0\">\r\n                  <div className=\"row align-items-stretch\">\r\n                    <div className=\"col-lg-6 p-lg-0\">\r\n                      <img\r\n                        alt=\"\"\r\n                        style={{ width: \"100%\" }}\r\n                        className=\"product-view d-block h-100 bg-cover bg-center\"\r\n                        